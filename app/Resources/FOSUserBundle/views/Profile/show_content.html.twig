{% trans_default_domain 'FOSUserBundle' %}

<div class="row">
  <div class="col-md-5">
      <button type="button" class="btn btn-primary btn-lg btn-block">Alhms for the Altar</button>
      <button type="button" class="btn btn-default btn-lg btn-block">Praise The Sun</button>
      <button type="button" class="btn btn-primary btn-lg btn-block">Multiplex</button>
      <button type="button" class="btn btn-primary btn-lg btn-block">Metaverse</button>
  </div>

  <div class="col-md-5">
      <div class="fos_user_user_show center-block">
          <p>{{ 'profile.show.username'|trans }}: {{ user.username }}</p>
          <p>{{ 'profile.show.email'|trans }}: {{ user.email }}</p>
      </div>
  </div>
</div>
